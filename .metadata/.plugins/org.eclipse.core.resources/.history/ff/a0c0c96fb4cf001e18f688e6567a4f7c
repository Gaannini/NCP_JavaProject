package liarGame;

import java.net.Socket;
import java.util.List;

import Server.Game;

public class LiarGame implements Game {
	private List<Socket> liarClients;

	public LiarGame() {
	}

	@Override
	public void start(Socket socket) {
		System.out.println("라이어게임을 선택하셧");
	}

	public List<Socket> getLiarClients() {
		return liarClients;
	}

	public void setLiarClients(List<Socket> liarClients) {
		this.liarClients = liarClients;
	}
	
	
}
